        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.2/dist/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script>
   function showBranding() {
     document.getElementById('carouselExampleControls1').style.display = 'block';
     document.getElementById('carouselExampleControls2').style.display = 'none';
     document.getElementById('brandingButton').classList.add('active');
     document.getElementById('automationButton').classList.remove('active');
     
     // Show branding images, hide automation images
     document.querySelectorAll('.branding-image').forEach(img => img.style.display = 'block');
     document.querySelectorAll('.automation-image').forEach(img => img.style.display = 'none');
   }

   function showAutomation() {
     document.getElementById('carouselExampleControls1').style.display = 'none';
     document.getElementById('carouselExampleControls2').style.display = 'block';
     document.getElementById('automationButton').classList.add('active');
     document.getElementById('brandingButton').classList.remove('active');
     
     // Show automation images, hide branding images
     document.querySelectorAll('.automation-image').forEach(img => img.style.display = 'block');
     document.querySelectorAll('.branding-image').forEach(img => img.style.display = 'none');
   }

   // Set initial state
   showBranding();
</script>


        
        
        

 


