<script>
document.addEventListener('DOMContentLoaded', function() {
    const canvas = document.getElementById('webgl-container');
    const scene = new THREE.Scene();
    const camera = new THREE.PerspectiveCamera(75, canvas.clientWidth / canvas.clientHeight, 0.1, 1000);
    const renderer = new THREE.WebGLRenderer({ canvas, antialias: true, alpha: true });
    renderer.setSize(canvas.clientWidth, canvas.clientHeight);

    const imageUrls = [
        'https://i.ibb.co/sy5CR68/Whats-App-Image-2024-07-09-at-23-09-16-7fe12d25-removebg-preview.png',
        'https://i.ibb.co/mFPBkVG/Whats-App-Image-2024-07-09-at-22-10-46-dd6f8648-removebg-preview.png',
        'https://i.ibb.co/b6bKxf0/Whats-App-Image-2024-07-09-at-23-00-00-81053a85-removebg-preview.png'
    ];

    let currentImageIndex = 0;
    let currentPlane;

    function loadTexture(url) {
        const loader = new THREE.TextureLoader();
        return new Promise((resolve, reject) => {
            loader.load(url, texture => resolve(texture), undefined, reject);
        });
    }

    function createPlane(texture, state) {
        const aspectRatio = texture.image.width / texture.image.height;
        const geometry = new THREE.PlaneGeometry(24 * aspectRatio, 24);
        const material = new THREE.MeshBasicMaterial({ map: texture, side: THREE.DoubleSide, transparent: true });
        const plane = new THREE.Mesh(geometry, material);
        scene.add(plane);

        if (state === 1) {
            const widthScale = 0.6; 
            const heightScale = 0.6; 
            plane.scale.set(widthScale, heightScale, 1); 
            plane.position.set(-1.9, -1.9, -7); 
        } else if (state === 2) {
            plane.scale.set(0.8, 0.8, 0.8);
            plane.position.set(0.5, 0.5, -7);
        } else {
            plane.scale.set(0.8, 0.8, 0.8);
            plane.position.set(0, 0, -7);
        }

        plane.rotation.y = 0;
        camera.position.z = 12;

        return plane;
    }

    function animate() {
        requestAnimationFrame(animate);
        renderer.render(scene, camera);
    }

    function onWindowResize() {
        const newWidth = canvas.clientWidth;
        const newHeight = canvas.clientHeight;
        camera.aspect = newWidth / newHeight;
        camera.updateProjectionMatrix();
        renderer.setSize(newWidth, newHeight);
    }

    window.addEventListener('resize', onWindowResize);

    function changeState(newState) {
        const titles = ['Live Projects', 'Onboarding', 'Completed'];
        const counters = ['01', '02', '03'];

        document.getElementById('step-title').style.opacity = 0;
        document.getElementById('counter').style.opacity = 0;
        canvas.style.opacity = 0;

        setTimeout(() => {
            document.getElementById('step-title').textContent = titles[newState];
            document.getElementById('counter').textContent = counters[newState];
            document.getElementById('step-title').style.opacity = 1;
            document.getElementById('counter').style.opacity = 1;

            loadTexture(imageUrls[newState]).then(texture => {
                if (currentPlane) {
                    scene.remove(currentPlane);
                }
                currentPlane = createPlane(texture, newState);
                canvas.style.opacity = 1;
            });
        }, 500);
    }

    function init() {
        loadTexture(imageUrls[0]).then(texture => {
            if (texture) {
                currentPlane = createPlane(texture, 0);
                animate();
            }
        }).catch(error => {
            console.error('Error loading texture:', error);
        });

        setInterval(() => {
            currentImageIndex = (currentImageIndex + 1) % 3;
            changeState(currentImageIndex);
        }, 5000);
    }

    init();
});
</script>

